{
  "extends": [
    "@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn",
    "no-console": "warn",
    "no-case-declarations": "off"
  },
  "ignorePatterns": [
    "dist/",
    "node_modules/"
  ]
}

